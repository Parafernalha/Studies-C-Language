/* Peça ao usuário para digitar três valores inteiros e imprima a soma deles.*/
int main () {
#include <stdio.h>
#include<stdlib.h>
int n1;
int n2;
int n3;
int valorsoma;
printf ("Ola, seja bem vindo\n");
printf ("Digite 3 numeros para serem somados.\n");
scanf ("%d %d %d", &n1, &n2, &n3);
valorsoma = n1 + n2 + n3;
printf("O valor da soma e igual a: %d\n", valorsoma);
}